api-1: MQ_NAMESPACE=multi MQ_CONSUMER_GROUP=api MQ_CONSUMER_GLOBAL=true PORT="${API_PORT:-3000}" NREPL_PORT="${API_NREPL_PORT:-7000}" API_BASE_URL="http://localhost:${API_PORT:-3000}" AUTH_BASE_URL="http://localhost:${AUTH_PORT:-3100}" UI_BASE_URL="http://localhost:${UI_PORT:-8080}" clj -J-XX:-OmitStackTraceInFastThrow -A:dev -m com.ben-allred.audiophile.backend.dev-server api
api-2: MQ_NAMESPACE=multi MQ_CONSUMER_GROUP=api MQ_CONSUMER_GLOBAL=true PORT="${API2_PORT:-3001}" NREPL_PORT="${API2_NREPL_PORT:-7001}" API_BASE_URL="http://localhost:${API2_PORT:-3001}" AUTH_BASE_URL="http://localhost:${AUTH2_PORT:-3101}" UI_BASE_URL="http://localhost:${UI2_PORT:-8081}" clj -J-XX:-OmitStackTraceInFastThrow -A:dev -m com.ben-allred.audiophile.backend.dev-server api
auth-1: PORT="${AUTH_PORT:-3100}" NREPL_PORT="${AUTH_NREPL_PORT:-7100}" AUTH_BASE_URL="http://localhost:${AUTH_PORT:-3100}" UI_BASE_URL="http://localhost:${UI_PORT:-8080}" clj -J-XX:-OmitStackTraceInFastThrow -A:dev -m com.ben-allred.audiophile.backend.dev-server auth
auth-2: PORT="${AUTH_PORT:-3101}" NREPL_PORT="${AUTH_NREPL_PORT:-7101}" AUTH_BASE_URL="http://localhost:${AUTH_PORT:-3101}" UI_BASE_URL="http://localhost:${UI_PORT:-8081}" clj -J-XX:-OmitStackTraceInFastThrow -A:dev -m com.ben-allred.audiophile.backend.dev-server auth
jobs-1: MQ_NAMESPACE=multi MQ_CONSUMER_GROUP=jobs PORT="${JOBS_PORT:-3200}" NREPL_PORT="${JOBS_NREPL_PORT:-7200}" clj -J-XX:-OmitStackTraceInFastThrow -A:dev -m com.ben-allred.audiophile.backend.dev-server jobs
jobs-2: MQ_NAMESPACE=multi MQ_CONSUMER_GROUP=jobs PORT="${JOBS_PORT:-3201}" NREPL_PORT="${JOBS_NREPL_PORT:-7201}" clj -J-XX:-OmitStackTraceInFastThrow -A:dev -m com.ben-allred.audiophile.backend.dev-server jobs
ui-1: PORT="${UI_PORT:-8080}" NREPL_PORT="${UI_NREPL_PORT:-7300}" API_BASE_URL="http://localhost:${API_PORT:-3000}" AUTH_BASE_URL="http://localhost:${AUTH_PORT:-3100}" UI_BASE_URL="http://localhost:${UI_PORT:-8080}" clj -J-XX:-OmitStackTraceInFastThrow -A:dev -m com.ben-allred.audiophile.backend.dev-server ui
ui-2: PORT="${UI_PORT:-8081}" NREPL_PORT="${UI_NREPL_PORT:-7301}" API_BASE_URL="http://localhost:${API_PORT:-3001}" AUTH_BASE_URL="http://localhost:${AUTH_PORT:-3101}" UI_BASE_URL="http://localhost:${UI_PORT:-8081}" clj -J-XX:-OmitStackTraceInFastThrow -A:dev -m com.ben-allred.audiophile.backend.dev-server ui
cljs: clj -A:cljs-dev:shadow-cljs -J-XX:-OmitStackTraceInFastThrow -m shadow.cljs.devtools.cli watch dev | grep --color=never -v DEBUG
sass: sass --watch src/scss/main.scss resources/public/css/main.css
