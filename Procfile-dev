api: PORT="${API_PORT:-3000}" NREPL_PORT="${API_NREPL_PORT:-7000}" API_BASE_URL="http://localhost:${API_PORT:-3000}" AUTH_BASE_URL="http://localhost:${AUTH_PORT:-3100}" EVENT_BASE_URL="http://localhost:${EVENT_PORT:-3200}" UI_BASE_URL="http://localhost:${UI_PORT:-8080}" clj -J-XX:-OmitStackTraceInFastThrow -A:dev -m com.ben-allred.audiophile.backend.dev-server api
auth: PORT="${AUTH_PORT:-3100}" NREPL_PORT="${AUTH_NREPL_PORT:-7100}" AUTH_BASE_URL="http://localhost:${AUTH_PORT:-3100}" UI_BASE_URL="http://localhost:${UI_PORT:-8080}" clj -J-XX:-OmitStackTraceInFastThrow -A:dev -m com.ben-allred.audiophile.backend.dev-server auth
events: PORT="${EVENT_PORT:-3200}" NREPL_PORT="${EVENT_NREPL_PORT:-7200}" clj -J-XX:-OmitStackTraceInFastThrow -A:dev -m com.ben-allred.audiophile.backend.dev-server events
jobs: PORT="${JOB_PORT:-3300}" NREPL_PORT="${JOB_NREPL_PORT:-7300}" clj -J-XX:-OmitStackTraceInFastThrow -A:dev -m com.ben-allred.audiophile.backend.dev-server jobs
ui: PORT="${UI_PORT:-8080}" NREPL_PORT="${UI_NREPL_PORT:-8180}" API_BASE_URL="http://localhost:${API_PORT:-3000}" AUTH_BASE_URL="http://localhost:${AUTH_PORT:-3100}" EVENT_BASE_URL="http://localhost:${EVENT_PORT:-3200}" UI_BASE_URL="http://localhost:${UI_PORT:-8080}" clj -J-XX:-OmitStackTraceInFastThrow -A:dev -m com.ben-allred.audiophile.backend.dev-server ui
cljs: clj -A:cljs-dev:shadow-cljs watch dev | grep --color=never -v DEBUG
sass: sass --watch src/scss/main.scss resources/public/css/main.css
