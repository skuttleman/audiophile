app: MQ_NAMESPACE=single MQ_CONSUMER_GROUP=app PORT="${API_PORT:-8080}" NREPL_PORT="${API_NREPL_PORT:-7000}" API_BASE_URL="http://localhost:${API_PORT:-8080}" AUTH_BASE_URL="http://localhost:${API_PORT:-8080}" UI_BASE_URL="http://localhost:${API_PORT:-8080}" clj -J-XX:-OmitStackTraceInFastThrow -A:dev -m com.ben-allred.audiophile.backend.dev-server api auth jobs ui
cljs: clj -A:cljs-dev:shadow-cljs -J-XX:-OmitStackTraceInFastThrow -m shadow.cljs.devtools.cli watch dev | grep --color=never -v DEBUG
sass: sass --watch src/scss/main.scss resources/public/css/main.css
